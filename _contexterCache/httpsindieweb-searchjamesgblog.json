   dark_style.media = \"all\";\n            li.innerHTML = \"<a onclick='toggleTheme();'>Light Mode</a>\";\n            localStorage.setItem('darkmode', 'true');\n          }\n        }\n\n        function trigger_modal(id) {\n            var modal = document.getElementById(id);\n            if (modal.style.display == \"none\") {\n                modal.style.display = \"block\";\n            } else {\n                modal.style.display = \"none\";\n            }\n        }\n      </script>\n      <div id=\"main\">\n          \n<section class=\"search\">\n  <img src=\"/static/indieweb.png\" alt=\"IndieWeb logo\" style=\"max-height: 200px;\" />\n  <h1>IndieWeb Search</h1>\n  \n    <p>Use the form below to search web sites published by members of the IndieWeb community and related sites.</p>\n  \n  <form action=\"/results\" method=\"GET\" role=\"search\">\n    <input type=\"search\" id=\"query\" name=\"query\" placeholder=\"Enter your query\" aria-label=\"Search\" autocomplete=\"off\" /><br>\n    <select id=\"suggestions\" style=\"display: none;\"></select>\n    \n    <br>\n    <input type=\"submit\" value=\"Search\">\n  </form>\n  <p class=\"advanced-p\">View <a href=\"/advanced\">advanced search options</a>.</p>\n  <h2>Explore New Topics</h2>\n  <ul class=\"topic_cloud\">\n    <li>\n      <a href=\"/results?query=what is webmention\">what is webmention</a>\n    </li>\n    <li>\n      <a href=\"/results?query=what is micropub\">what is micropub</a>\n    </li>\n    <li>\n      <a href=\"/results?query=feed reader\">feed reader</a>\n    </li>\n    <li>\n      <a href=\"/results?query=microformats\">microformats</a>\n    </li>\n    <li>\n      <a href=\"/results?query=make a website\">make a website</a>\n    </li>\n    <li>\n      <a href=\"/results?query=rss tutorial\">rss tutorial</a>\n    </li>\n    <li>\n      <a href=\"/results?query=eleventy\">eleventy</a>\n    </li>\n    <li>\n      <a href=\"/results?query=digital gardens\">digital gardens</a>\n    </li>\n  </ul