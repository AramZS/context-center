ft: 0;\n            width: 100%;\n            height: 100%;\n            background: linear-gradient(135deg, rgba(139, 0, 0, 0.1) 0%, rgba(139, 69, 19, 0.1) 50%, rgba(255, 140, 0, 0.05) 100%);\n            z-index: -1;\n        }\n\n        .container {\n            max-width: 1200px;\n            margin: 0 auto;\n            padding: 2rem;\n        }\n\n        .header {\n            text-align: center;\n            color: #d4af37;\n            margin-bottom: 3rem;\n        }\n\n        .header h1 {\n            font-size: 3rem;\n            font-weight: 700;\n            margin-bottom: 0.5rem;\n            text-shadow: 2px 2px 4px rgba(139, 0, 0, 0.5);\n            color: #d4af37;\n        }\n\n        .header .subtitle {\n            font-size: 1.3rem;\n            font-weight: 400;\n            opacity: 0.9;\n            margin-bottom: 0.5rem;\n        }\n\n        .header .creator {\n            font-size: 1.1rem;\n            font-weight: 400;\n            margin: 1rem 0;\n        }\n\n        .header .creator a {\n            color: #ff8c42;\n            text-decoration: none;\n            border-bottom: 2px solid transparent;\n            transition: border-color 0.3s ease;\n        }\n\n        .header .creator a:hover {\n            border-bottom-color: #ff8c42;\n        }\n\n        .header .conference {\n            font-size: 1.2rem;\n            font-weight: 400;\n            margin-top: 1rem;\n            opacity: 0.8;\n        }\n\n        .header .conference a {\n            color: #d4af37;\n            text-decoration: none;\n            border-bottom: 2px solid transparent;\n            transition: border-color 0.3s ease;\n        }\n\n        .header .conference a:hover {\n            border-bottom-color: #d4af37;\n        }\n\n        .content-grid {\n            display: grid;\n            grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));\n            gap: 2rem;\n            margin-bottom: 3rem;\n        }\n\n        .card {\n            background: rgba(0, 0, 0, 0.85);\n            border: 2px solid #8b4513;\n            border-radius: 15px;\n            padding: 2rem;\n            box-shadow: 0 0 30px rgba(139, 0, 0, 0.3);\n            transition: transform 0.3s ease, box-shadow 0.3s ease;\n        }\n\n        .card:hover {\n            transform: translateY(-5px);\n            box-shadow: 0 0 40px rgba(255, 140, 0, 0.4);\n        }\n\n        .card h2 {\n            color: #d4af37;\n            font-size: 1.5rem;\n            font-weight: 600;\n            margin-bottom: 1rem;\n            border-bottom: 2px solid #8b4513;\n            padding-bottom: 0.5rem;\n            text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.8);\n        }\n\n        .card h3 {\n            color: #ff8c42;\n            font-size: 1.2rem;\n            font-weight: 600;\n            margin: 1.5rem 0 0.5rem 0;\n        }\n\n        .link-list {\n            list-style: none;\n        }\n\n        .link-list li {\n            margin: 0.75rem 0;\n        }\n\n        .link-list a {\n            display: flex;\n            align-items: center;\n            padding: 0.75rem 1rem;\n            background: rgba(139, 69, 19, 0.2);\n            border-radius: 8px;\n            text-decoration: none;\n            color: #d4af37;\n            transition: all 0.3s ease;\n            border-left: 4px solid #ff8c42;\n            border: 1px solid #8b4513;\n        }\n\n        .link-list a:hover {\n            background: rgba(139, 69, 19, 0.4);\n            transform: translateX(5px);\n            color: #fff;\n            box-shadow: 0 4px 15px rgba(255, 140, 0, 0.3);\n        }\n\n        .link-list .icon {\n            width: 20px;\n            height: 20px;\n            margin-right: 0.75rem;\n            opacity: 0.7;\n        }\n\n        .demo-link {\n            background: linear-gradient(135deg, #8b0000, #ff6b35) !important;\n            color: #f5f5dc !important;\n            border-left: 4px solid #ff5252 !important;\n            border: 2px solid #8b4513 !important;\n        }\n\n        .demo-link:hover {\n            background: linear-gradient(135deg, #ff6b35, #daa520) !important;\n            transform: translateX(5px) translateY(-2px) !important;\n        }\n\n        .tools-list {\n            display: grid;\n            grid-template-columns: repeat(auto-fit, minmax(120px, 1fr));\n            gap: 1rem;\n            margin-top: 1rem;\n        }\n\n        .tool-item {\n            text-align: center;\n            padding: 1rem;\n            background: rgba(139, 69, 19, 0.2);\n            border-radius: 8px;\n            border: 2px solid #8b4513;\n            transition: all 0.3s ease;\n        }\n\n        .tool-item:hover {\n            border-color: #ff8c42;\n            transform: translateY(-2px);\n            box-shadow: 0 4px 15px rgba(255, 140, 0, 0.3);\n        }\n\n        .tool-item .tool-name {\n            font-weight: 600;\n            color: #d4af37;\n            margin-top: 0.5rem;\n        }\n\n        .description {\n            color: #c9b037;\n            font-size: 0.95rem;\n            margin-bottom: 1.5rem;\n            line-height: 1.7;\n        }\n\n        .footer {\n            text-align: center;\n            color: #d4af37;\n            opacity: 0.8;\n            margin-top: 2rem;\n            padding-top: 2rem;\n            border-top: 1px solid rgba(139, 69, 19, 0.4);\n        }\n\n        @media (max-width: 768px) {\n            .container {\n                padding: 1rem;\n            }\n            \n            .header h1 {\n                font-size: 2.5rem;\n            }\n            \n            .header .subtitle {\n                font-size: 1.3rem;\n            }\n            \n            .content-grid {\n                grid-template-columns: 1fr;\n            }\n            \n            .tools-list {\n                grid-template-columns: 1fr;\n            }\n        }\n    </style>\n</head>\n<body>\n    <div class=\"container\">\n        <header class=\"header\">\n            <h1>Bots Building Bots</h1>\n            <div class=\"subtitle\">To the Funhouse and Beyond: Probing Generative AI for Playful Text Generation</div>\n            <div class=\"creator\"><a href=\"https://bsky.app/profile/anasalter.bsky.social\" target=\"_blank\">Anastasia Salter</a></div>\n            <div class=\"conference\"><a href=\"https://elo25.org/\" target=\"_blank\">ELO 2025 Conference</a></div>\n        </header>\n\n        <div class=\"content-grid\">\n            <div class=\"card\">\n                <h2>üé≠ Demo</h2>\n                <p class=\"description\">\n                    A simple demo of output from a GPT2 trained on Dante's <i>Inferno.</i>\n                </p>\n                <ul class=\"link-list\">\n                    <li>\n                        <a href=\"dante.html\" class=\"demo-link\">\n                            <span class=\"icon\">üî•</span>\n                            <span>Modified GPT2 Demo Output</span>\n                        </a>\n                    </li>\n                </ul>\n            </div>\n\n            <div class=\"card\">\n                <h2>üî¨ Process</h2>\n                <p class=\"description\">\n                    Python code generated using Claude Sonnet 4.\n                </p>\n                <ul class=\"link-list\">\n                    <li>\n                        <a href=\"https://colab.research.google.com/drive/1cmsALgpYRbqZuoJWAbeWZ1Cytq3dbpDs?usp=sharing\" target=\"_blank\">\n                            <span class=\"icon\">üìì</span>\n                            <span>The Notebook (Google Colab)</span>\n                        </a>\n                    </li>\n                    <li>\n                        <a href=\"https://huggingface.co/anasalter/dante-inferno-gpt2\" target=\"_blank\">\n                            <span class=\"icon\">ü§ó</span>\n                            <span>The Trained Bot (Hugging Face)</span>\n                        </a>\n                    </li>\n                </ul>\n            </div>\n\n            <div class=\"card\">\n                <h2>üõ†Ô∏è Toolkit</h2>\n                <p class=\"description\">\n                    Google Colab is not required, but is helpful for pedagogy and working with LLMs without installing Python locally.\n                </p>\n                <div class=\"tools-list\">\n                    <div class=\"tool-item\">\n                        <div style=\"font-size: 2rem;\">ü§ñ</div>\n                        <div class=\"tool-name\">Claude Sonnet 4</div>\n                    </div>\n                    <div class=\"tool-item\">\n                        <div style=\"font-size: 2rem;\">üî¨</div>\n                        <div class=\"tool-name\">Google Colab</div>\n                    </div>\n                    <div class=\"tool-item\">\n                        <div style=\"font-size: 2rem;\">ü§ó</div>\n                        <div class=\"tool-name\">Hugging Face</div>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"card\">\n                <h2>üìö Additional Resources</h2>\n                <p class=\"description\">\n                    More materials for exploring AI in the humanities and digital programming approaches.\n                </p>\n                <ul class=\"link-list\">\n                    <li>\n                        <a href=\"https://anastasiasalter.net/HumanitiesAI/\" target=\"_blank\">\n                            <span class=\"icon\">üß†</span>\n                            <span>Humanities in the Age of AI - Graduate Syllabus</span>\n                        </a>\n                    </li>\n                    <li>\n                        <a href=\"https://anastasiasalter.net/Creative-Coding/\" target=\"_blank\">\n                            <span class=\"icon\">üé®</span>\n                            <span>Creative Coding & GenAI - Undergraduate Syllabus</span>\n                        </a>\n                    </li>\n                    <li>\n                        <a href=\"https://anastasiasalter.net/DHProgramming/\" target=\"_blank\">\n                            <span class=\"icon\">üíª</span>\n                            <span>DH Programming (DHSI Institute)</span>\n                        </a>\n                    </li>\n                    <li>\n                        <a href=\"https://docs.google.com/presentation/d/1-IuScE1OBeu9iP2TAUTW1RUoHtAak4bV/edit?usp=sharing&ouid=115206709767153443873&rtpof=true&sd=true\" target=\"_blank\">\n                            <span class=\"icon\">üìä</span>\n                            <span>ELO Slides - Feminist Platforms</span>\n                        </a>\n                    </li>\n                    <li>\n                        <a href=\"https://docs.google.com/presentation/d/16DwgLPAfoQ0z6U9h7Dl3v0r5jZiijII_/edit?usp=sharing&ouid=115206709767153443873&rtpof=true&sd=true\" target=\"_blank\">\n                            <span class=\"icon\">üîÆ</span>\n                            <span>ELO Slides - GenAI Futures</span>\n                        </a>\n                    </li>\n                </ul>\n            </div>\n        </div>\n\n        <footer class=\"footer\">\n            <p><a href=\"https://elo25.org/\" target=\"_blank\" style=\"color: #d4af37; text-decoration: none;\">ELO 2025 Conference</a> ‚Ä¢ Bots Building Bots</p>\n      \n        </footer>\n    </div>\n</body>\n</html>\n","oembed":false,"readabilityObject":{"title":"ELO 2025 - Bots Building Bots Workshop","content":"<div id=\"readability-page-1\" class=\"page\"><div>\n                <h2>üõ†Ô∏è Toolkit</h2>\n                <p>\n                    Google Colab is not required, but is helpful for pedagogy and working with LLMs without installing Python locally.\n                </p>\n                <div>\n                    <div>\n                        <p>ü§ñ</p>\n                        <p>Claude Sonnet 4</p>\n                    </div>\n                    <div>\n                        <p>üî¨</p>\n                        <p>Google Colab</p>\n                    </div>\n                    <div>\n                        <p>ü§ó</p>\n                        <p>Hugging Face</p>\n                    </div>\n                </div>\n            </div></div>","textContent":"\n                üõ†Ô∏è Toolkit\n                \n                    Google Colab is not required, but is helpful for pedagogy and working with LLMs without installing Python locally.\n                \n                \n                    \n                        ü§ñ\n                        Claude Sonnet 4\n                    \n                    \n                        üî¨\n                        Google Colab\n                    \n                    \n                        ü§ó\n                        Hugging Face\n                    \n                \n            ","length":565,"excerpt":"Google Colab is not required, but is helpful for pedagogy and working with LLMs without installing Python locally.","byline":null,"dir":null,"siteName":null,"lang":"en"},"finalizedMeta":{"title":"ELO 2025 - Bots Building Bots Workshop","description":"Google Colab is not required, but is helpful for pedagogy and working with LLMs without installing Python locally.","author":false,"creator":"","publisher":false,"date":"2025-07-27T22:59:49.055Z","topics":[]},"jsonLd":{"@type":false,"headline":false,"description":false,"image":[],"mainEntityOfPage":{"@type":false,"@id":false},"datePublished":false,"dateModified":false,"isAccessibleForFree":false,"isPartOf":{"@type":[],"name":false,"productID":false},"discussionUrl":false,"license":false,"author":{"@type":false,"name":false,"description":false,"sameAs":false,"image":{"@type":false,"url":false},"givenName":false,"familyName":false,"alternateName":false,"publishingPrinciples":false},"publisher":{"@type":false,"name":false,"descripti